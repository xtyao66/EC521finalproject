{
  "manifest_version": 2,
  "name": "URL Checker",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*/*"
  ],
  "web_accessible_resources": [
    "localHandler.html", "handler.js", "warning.html", "warning.js", "hello.html", "bootstrap.min.css", "cryptoHandler.html", "crypto.js"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
